          <TableColumn fx:id="tableColumnID" prefWidth="26.0" text="ID" />
          <TableColumn fx:id="tableColumnBrand" prefWidth="54.0" text="Brand" />
            <TableColumn fx:id="tableColumnModel" prefWidth="65.0" text="Model" />
            <TableColumn fx:id="tableColumnRam" prefWidth="32.0" text="Ram" />
            <TableColumn fx:id="tableColumnProcessor" prefWidth="68.0" text="Processor" />
            <TableColumn fx:id="tableColumnScreen" prefWidth="43.0" text="Screen" />
            <TableColumn fx:id="tableColumnPrice" prefWidth="39.0" text="Price" />
        </columns>
      </TableView>
    </children>

</AnchorPane>
